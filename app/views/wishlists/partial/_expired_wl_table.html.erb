<% @expired_wishlists.each do |wishlist| %>
  <table class="table table-condensed " style="border-collapse:collapse;">
    <thead>
      <tr>
        <th>&nbsp;</th>
        <th>Artwork</th>
        <th>Book</th>
        <th>Subject</th>
        <th>Year</th>
        <th>Type</th>
        <th>Target Price</th>
        <th>Actions</th>
        <!--# of bids, max min? price diff or best? idk...-->
      </tr>
    </thead>
    <tbody>
      <!-- No need for collapsing and complex tables here. Only 1 per book -->
      <tr>
        <td>&nbsp;</td>
        <td><img src="<%= wishlist.book.artwork %>" height=30%></td>
        <td><%= wishlist.book.title %></td>
        <td><%= wishlist.book.subject.subject_number %> - <%= wishlist.book.subject.subject_name %></td>
        <td><%= wishlist.book.year %></td>
        <td><%= wishlist.sale_rent %></td>
        <td><%= number_to_currency(wishlist.price) %></td>
        <td>
          <span title= "Dismiss"><a href="/delete_wishlist/<%= wishlist.id %>" class="btn btn-xs btn-warning" rel="nofollow"><i class="fa fa-eye-slash"></i></a></span>
        </td>
      </tr>
    </tbody>
  </table>
<% end %>
